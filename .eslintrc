{
  "extends": "airbnb",
  "parser": "babel-eslint",

  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },

  "globals": {
    "$": true,
    "swal": true,
  },

  "rules": {
    "id-length": 0,
    "indent": ["error", 2, {"SwitchCase": 1}],
    "no-console": 0,
    "no-plusplus": 0,
    "func-names": 0,
    "no-use-before-define": 0,
    "linebreak-style": 0,
    "import/no-extraneous-dependencies": 0,
    "react/no-multi-comp": 0,
    "react/prop-types": 0,
    "react/jsx-indent": ["error", 2],
    "jsx-a11y/no-static-element-interactions": 0
  }
}
